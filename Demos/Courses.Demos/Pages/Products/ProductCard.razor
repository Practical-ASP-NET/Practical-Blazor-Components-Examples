@using Courses.Demo.Shared

<div class="card">
    <div class="card-body">
        <h5 class="card-title">@Product.Name</h5>
        <p class="card-text">@Product.Description</p>
        <p>£@Product.Price</p>
    </div>
</div>

@code {
    
    [Parameter]
    public Product Product { get; set; }
    
}